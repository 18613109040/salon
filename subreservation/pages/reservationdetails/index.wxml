<view class="big_box">
  <view class='title'>
    <view class='title_one'>
      <text>{{reservationOrderDetail.orderStatusText}}</text>
    </view>
    <view class='title_tow' wx:if="{{reservationOrderDetail.orderStatus==21}}">
      <text class='gray'>请在下单后</text>
      <text class='bule'>{{time}}</text>
      <text class='gray'>分钟内付款，超时自动取消</text>
    </view>
    <view class="indent_nav_content wellcome" wx:if="{{reservationOrderDetail.orderStatus==22&&reservationOrderDetail.orderType==2}}">
      <text class='right_title_noe'>商家恭候您的莅临</text>
      <text class='right_title_tow'>请在项目完成后支付商家费用</text>
    </view>
    <view class="indent_nav_content wellcome" wx:if="{{reservationOrderDetail.orderStatus==29&&reservationOrderDetail.orderType!=2}}">
      <text class='right_title_noe'>订单时间已过期</text>
      <text class='right_title_tow'>订单将自动取消，金额原路返回用户</text>
    </view>
    <view class="indent_nav_content linheight" wx:if="{{reservationOrderDetail.orderStatus==29&&reservationOrderDetail.orderType==2}}">
      订单时间已过期
    </view>
    <view class="indent_nav_content linheight" wx:if="{{reservationOrderDetail.orderStatus==23}}">
      <text class="indent_nav_content_tiem">卷码已使用</text>
    </view>
    <view class="indent_nav_content linheight" wx:if="{{reservationOrderDetail.orderStatus==25}}">
      客户已申请退款，等待商家处理
    </view>
    <view class="indent_nav_content linheight" wx:if="{{reservationOrderDetail.orderStatus==27}}">
      退款将在7个工作日内到账
    </view>
    <view class="indent_nav_content linheilinhei" wx:if="{{reservationOrderDetail.orderStatus==31}}">
      用户已取消
    </view>
    <view class="indent_nav_content linhei" wx:if="{{reservationOrderDetail.orderStatus==22&&reservationOrderDetail.orderType!=2}}">
      券码有效期至
      <text class="indent_nav_content_tiem">{{reservationOrderDetail.couponExpireDt}}</text>
    </view>
  </view>
  <view class='comment'>
    <component-shopmap shopInfo="{{shopInfo}}" type="2" />
    <component-line/>
  </view>
  <view class='bottom'>
    <view class='time'>
      <text class='time_tie'>预约时间</text>
      <text class='week_day' space="true" decode="true">{{reservationOrderDetail.subscribeDtStr}} </text>
    </view>
    <view class='time'>
      <text class='time_tie'>预约项目</text>
      <text class='week_day'>{{reservationOrderDetail.packageName}}</text>
    </view>
    <view class='time'>
      <text class='time_tie'>项目信息</text>
      <text wx:if="{{reservationOrderDetail.skuValue}}">{{reservationOrderDetail.skuValue}}</text>
      <text class='week_day'>{{reservationOrderDetail.serverName}}</text>
    </view>
    <view class='time'>
      <text class='time_tie'>到店信息</text>
      <text class='week_day' space="true" decode="true">{{reservationOrderDetail.packageQuantity}}人&nbsp;|&nbsp;{{reservationOrderDetail.buyerPhone}} </text>
    </view>

    <view class='time'>
      <text class='time_tie '>备注留言</text>
      <text class='beizhu'>{{reservationOrderDetail.remark}}</text>
    </view>
    <view class='erwm_box'>
      <view class='yuerwm'>
        <text>预约码</text>
      </view>
      <view class='erwm_img' wx:for="{{reservationOrderDetail.pickCode}}" wx:key="{{index}}">
        <view style='margin:0px auto;text-align: center;'>
          <canvas style="width:300rpx;height:300rpx;margin-left:{{windowWidth/2-150}}rpx" canvas-id="mycanvas{{index}}" />
        </view>
        <text class='num'>{{item.pickCode}}</text>
      </view>
    </view>
    <view class='btn_box' wx:if="{{reservationOrderDetail.orderStatus==22}}">
      <view class='btn_list'>
        <view class='btn_noe' bindtap='phonecallevent'>
          <image class='call' src='/images/phone_gray@2x.png'></image>
          <text>联系商家</text>
        </view>
        <view class='btn_tow' bindtap='cancelReservation'>
          <text>取消预约</text>
        </view>
      </view>
    </view>


    
     <view class='btn_box' wx:if="{{reservationOrderDetail.orderStatus==26}}">
    <view class='btn_list'>
      <view class='btn_noe' bindtap='phonecallevent'>
        <image class='call' src='/images/phone_gray@2x.png'></image>
        <text>联系商家</text>
      </view>
      <view class='btn_tow btn_jujue' bindtap="submit">
        <text class='txt'>商家拒绝退款</text>
        <text class='txt texone'> 查看原因</text>
      </view>
    </view>
  </view>


    <view class='btn_box' wx:if="{{reservationOrderDetail.orderStatus==23&&reservationOrderDetail.evaluateStatus!=2}}">
      <view class='btn_list'>
        <view class='btn_son' bindtap='evaluation'>
          <image class='call' src='/images/phone_gray@2x.png'></image>
          <text>立即评价</text>
        </view>
      </view>
    </view>
    <view class='btn_box' wx:if="{{reservationOrderDetail.orderStatus==23&&reservationOrderDetail.evaluateStatus==2}}">
      <view class='btn_list'>
        <view class='btn_son' bindtap='evaluation'>
          <image class='call' src='/images/phone_gray@2x.png'></image>
          <text>查看评价</text>
        </view>
      </view>
    </view>

    <!-- <view class='btn_box' wx:if="{{reservationOrderDetail.orderStatus==21}}">
      <view class='btn_list btn_farher'>
        <view class='one_box'>
          <text>合计:￥288</text>
        </view>
        <view class='tow_box' bindtap='clickCancelOrder'>
          <text>取消订单</text>
        </view>
        <view class='tree_box' bindtap='payImmediately'>
          <text>立即支付</text>
        </view>
      </view>
    </view> -->
  </view>
</view>
<view class="mask" bindtap="go" catchtouchmove="preventTouchMove" wx:if="{{showModal}}"></view>

<view class="modalDlg" wx:if="{{showModal}}">
  <image src="/figures/logo-smile.png" />
  <text>66666~</text>
  <button>66666</button>
</view>