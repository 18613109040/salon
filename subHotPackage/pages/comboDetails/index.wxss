.project_list {
  width: 100%;
  position: relative;
  background-color: #fff;
  box-sizing: border-box;
  overflow: hidden;
}

.swiper {
  height: 670rpx;
}

.project_list .project_img {
  width: 100%;
  height: 670rpx;
}

.project_list .project_image {
