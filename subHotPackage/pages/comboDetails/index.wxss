  left: 10rpx;
}

.project_left {
  display: flex;
  padding-top: 20rpx;
  align-items: center;
}

.project_right {
  font-size: 12px;
  color: #9b9b9b;
  flex-flow: 120rpx;
  text-align: right;
}

.project_left_title {
  height: 50rpx;
  font-size: 36rpx;
  color: #6fa7e8;
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.project_list {
  padding: 20rpx 30rpx 0 30rpx;
}

.project .project_title .project_right .project_right_old {
