/* 预约时间  */

.sb-drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.5);
  overflow: hidden;
}

.sb-drawer_pick {
  overflow: hidden;
  position: fixed;
  z-index: 1001;
  background: #fff;
  padding: 20rpx 0rpx 20rpx 0;
  width: 100%;
  bottom: 0;
}

.pick-header {
  display: flex;
  padding: 20rpx 20rpx 20rpx 20rpx;
}

.pick-header .cancel {
  flex: 1;
  text-align: left;
}

.pick-header .enter {
  flex: 1;
  text-align: right;
  color: #ff7920;
}

.picker_center {
  text-align: center;
}

.picker_img {
  width: 43px;
  height: 45px;
}

.waiter_box_images {
  width: 43px;
  height: 45px;
  margin-top: 10rpx;
}

.waiter_box_images_box {
  line-height: 50px;
  flex: 2;
}

.pircker_ico {
  position: absolute;
  border: 1px solid #ccc;
  height: 19px;
  width: 19px;
  border-radius: 50%;
  margin-top: 10rpx;
  right: 0;
  top: 17rpx;
}

.waiter_name {
  flex: 4;
}

.pircker_ico_box {
  flex: 4;
  text-align: right;
}

.waiter_box {
  position: relative;
  display: flex;
  line-height: 50px;
  margin-left: 20rpx;
  margin-right: 20rpx;
}



.drawer {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.5);
  overflow: hidden;
}

.drawer_box {
  position: fixed;
  top: 20rpx;
  z-index: 1001;
  background: #fff;
  margin: 0 20rpx;
  width: 700rpx;
  height: 1044rpx;
}

.drawer_box-header {
  padding: 24rpx 0;
  font-size: 32rpx;
  color: #4a4a4a;
  text-align: center;
  position: relative;
}

.close-drawer {
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}

.close-drawer image {
  width: 34rpx;
  height: 34rpx;
}

.drawer_box-character {
  margin: 0 24rpx;
  border-bottom: 1px solid #f3f3f3;
  border-top: 1px solid #f3f3f3;
  padding: 20rpx 0;
}

.drawer_box-tip {
  font-size: 24rpx;
  color: #000;
  padding: 16rpx 40rpx;
}


.drawer_box-box {
  padding: 0 30rpx;
}


.scroll-list {
  height: 920rpx;
}

.item {
  display: flex;
  padding: 20rpx;
  align-items: center;
}

.avatar {
  width: 90rpx;
  height: 90rpx;
  border-radius: 100%;

}

.nickname {
 font-size: 28rpx;
 color: #4A4A4A ;
 margin-left: 10rpx;
}
.alias{
  font-size: 24rpx;
margin-left: 10rpx;
}
.radio {


}
.radio_no{
  border: 1px solid #9B9B9B;
  border-radius: 100%;
  height: 40rpx;
  width: 40rpx;

}
.left-item{
  flex: 1;
  display: flex;
  align-items: center;
}

.selected {
  background: #877fd4;
}

.drawer_box-more image {
  width: 40rpx;
  height: 20rpx;
}

.ok {
  position: absolute;
  bottom: 0;
  height: 110rpx;
  line-height: 110rpx;
  background: #877fd4;
  text-align: center;
  z-index: 1000;
  width: 100%;
  color: #fff;
}
.list_conter_tiem {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}