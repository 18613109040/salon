.view {
  box-sizing: border-box;
  background: #fff;
  border-top: 1px solid #eee;
}

.view_box {
  margin-left: 40rpx;
  margin-right: 40rpx;
}

.icon-arrow-r::after {
  content: "\e602";
  font-size: 40rpx;
}

.icon-add::after {
  content: '\e6c9';
  font-size: 50rpx;
  color: #4a4a4a;
}

.icon-desc::after {
  content: '\e6ca';
  font-size: 50rpx;
  color: #4a4a4a;
}

