
.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.icon-star:after{
  content: '\e639';
}
.rate{
    margin: 0;
    padding: 0;
    list-style: none;
    /* display: inline-block; */
    vertical-align: middle;
    font-weight: normal;
    font-style: normal;
    display: flex;
    align-items: center;
}
.rate-first {
  transition: all 0.3s;
}

.rate-second {
  transition: all 0.3s;
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  overflow: hidden;
  opacity: 1;
}

.rate-full .rate-second{
  color: #fec12b;
}

.rate-half .rate-second{
  color: #fec12b;
}
.rate-star {
  margin: 0;
  padding: 0;
  display: inline-block;
  margin-right: 8rpx;
  position: relative;
  transition: all 0.3s;
  color: #dbdbdb;
}

.rate-1 .rate-second{
  width: 10%;
}

.rate-2 .rate-second{
  width: 20%;
}

.rate-3 .rate-second{
  width: 30%;
}

.rate-4 .rate-second{
  width: 40%;
}

.rate-5 .rate-second{
  width: 50%;
}

.rate-6 .rate-second{
  width: 60%;
}

.rate-7 .rate-second{
  width: 70%;
}

.rate-8 .rate-second{
  width: 80%;
}

.rate-9 .rate-second{
  width: 90%;
}
