.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.icon-colose:after{
  content: '\e666';
  font-size: 40rpx;
}
.mask{
   position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 19;
  background: rgba(0, 0, 0, 0.3);
}
.model-content{
  position: fixed;
  z-index: 20;
  width:94%;
  max-height: 96%;
  min-height: 50%;
  background: white;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  
}
.model-content .header{
  display: flex;
  align-items: center;
  padding: 30rpx;
}
.model-content .header .title{
  flex: 1;
  text-align: center;
}
.model-content .header .close-btn{
  flex-basis: 40rpx;
}
.model-content .header .title .name{
  font-size: 32rpx;
  color: #000000;
}
.model-content .header .title .des{
  color: #4A4A4A;
  font-size: 24rpx;
}